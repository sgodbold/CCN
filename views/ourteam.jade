extends layout

block team

  .area-content
    .container
      .row.bottom-margin(style='margin-bottom: 10px;')
        h1.text-center Our Team
      .row

        #danielBloch.modal.fade(tabindex='-1', role='dialog', aria-labelledby='myModalLabel', aria-hidden='true')
          .modal-dialog
            .modal-content
              .modal-header
                button.close(type='button', data-dismiss='modal', aria-hidden='true') ×
                h4#myModalLabel.modal-title Daniel Bloch, Operations
              .modal-body
                | Information on Bloch...
              .modal-footer
                button.btn.btn-default(type='button', data-dismiss='modal') Close
