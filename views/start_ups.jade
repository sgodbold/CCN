extends layout

block blog

  .area-content
    .container
      .row.bottom-margin(style='margin-bottom: 10px;')
        h1.text-center Start Up Central
      .row
        .fluid-container(style='padding-left: 15px; padding-right: 15px;')
          ul(style='list-style: none;')
            each post, i in posts
              li
                 a(href="/community/start_ups/"+post.title)= post.title
        .clearfix
        .clearfix